{
    "manifest_version": 3,
    "name": "FBCLID Cleaner",
    "version": "1.0",
    "description": "Auto-remove the fbclid parameter from URLs",
    "permissions": ["tabs", "webNavigation", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_title": "FBCLID Cleaner activated"
    }
  }